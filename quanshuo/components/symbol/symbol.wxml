<!--components/symbol/symbol.wxml-->
<view class="container">       
  <view class="table" wx:for="{{10}}" wx:key="id">
      <view class="td" bindtap='openCard' >内容</view>
      <view class="td" bindtap='openCard' >内容</view>
      <view class="td" bindtap='openCard' >内容</view>
      <view class="td" bindtap='openCard' >内容</view>
      <view class="td" bindtap='openCard' >内容</view>
  </view>

  <view class='switchView'> 
      <input placeholder="片假名" placeholder-class="input-placeholder" disabled="{{disabled}}" />     
      <view class="switchBtn" 
            bindtouchstart="moveSendBtnStart" 
            bindtouchend="moveSendBtnEnd" 
            bindtouchmove="moveSendBtn" 
            style="left:{{moveSendBtnLeft}}rpx;background-color:{{SendBtnColor}}">
            平假名
      </view>      
  </view>
</view>