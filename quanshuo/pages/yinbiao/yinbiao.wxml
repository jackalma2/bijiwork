<view>
  <view class='navigator-bg'>
    <button id="0" size='mini' bindtap='changeSymbol' >test1</button>
    <button id="1" size='mini' bindtap='changeSymbol' >test2</button>
    <button id="2" size='mini' bindtap='changeSymbol' >test3</button>
  </view>

  <view class='swiper-symbol'>
    <swiper current="{{current}}" 
            indicator-dots="{{indicatorDots}}" 
            autoplay="{{autoplay}}" 
            interval="{{interval}}" 
            duration="{{duration}}"
            circular="{{circular}}"
    >

      <swiper-item>
        <component-tag-name inner-text="some text" bindmyevent="onMyEvent">
          <!-- 这部分内容将被放置在组件 <slot> 的位置上 -->         
        </component-tag-name>
      </swiper-item>

       <swiper-item>
        <view>
          <text>aaaaaa</text>
        </view>
      </swiper-item>

       <swiper-item>
        <component-tag-name inner-text="some text" bindmyevent="onMyEvent">
          <!-- 这部分内容将被放置在组件 <slot> 的位置上 -->         
        </component-tag-name>
      </swiper-item>

    </swiper>
  </view>
</view>